{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/ai-automation",
      "destination": "/ai-automation.html"
    },
    {
      "source": "/web-development",
      "destination": "/web-development.html"
    },
    {
      "source": "/ecommerce-development",
      "destination": "/ecommerce-development.html"
    },
    {
      "source": "/social-media-branding",
      "destination": "/social-media-branding.html"
    },
    {
      "source": "/favicon.ico",
      "destination": "/public/favicon.ico"
    },
    {
      "source": "/favicon.svg",
      "destination": "/public/favicon.svg"
    },
    {
      "source": "/favicon-96x96.png",
      "destination": "/public/favicon-96x96.png"
    },
    {
      "source": "/apple-touch-icon.png",
      "destination": "/public/apple-touch-icon.png"
    },
    {
      "source": "/android-chrome-192x192.png",
      "destination": "/public/android-chrome-192x192.png"
    },
    {
      "source": "/android-chrome-512x512.png",
      "destination": "/public/android-chrome-512x512.png"
    },
    {
      "source": "/site.webmanifest",
      "destination": "/public/site.webmanifest"
    },
    {
      "source": "/logo.png",
      "destination": "/public/logo.png"
    },
    {
      "source": "/logo-white.png",
      "destination": "/public/logo-white.png"
    },
    {
      "source": "/robots.txt",
      "destination": "/public/robots.txt"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/public/sitemap.xml"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/public/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}